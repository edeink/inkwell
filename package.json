{
  "name": "@edeink/inkwell",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "format": "stylelint --fix \"src/**/*.{css,less}\" && eslint --ext .js,.jsx,.ts,.tsx src/ --fix && prettier --write \"src/**/*.{js,jsx,ts,tsx,css,less}\"",
    "preview": "vite preview",
    "test": "vitest run",
    "benchmark": "vite --open /src/benchmark/index.html",
    "dev:mindmap": "vite --open /src/demo/mindmap/index.html",
    "doc": "docusaurus start",
    "doc:build": "docusaurus build",
    "doc:serve": "docusaurus serve",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "classnames": "^2.5.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.1",
    "@babel/standalone": "^7.26.3",
    "@docusaurus/core": "^3.2.1",
    "@docusaurus/preset-classic": "^3.2.1",
    "@docusaurus/theme-live-codeblock": "^3.2.1",
    "@docusaurus/theme-mermaid": "^3.9.2",
    "@docusaurus/tsconfig": "^3.9.2",
    "@docusaurus/types": "^3.9.2",
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "antd": "^5.29.0",
    "baseline-browser-mapping": "^2.8.31",
    "css-loader": "^6.10.0",
    "docusaurus-plugin-search-local": "^2.1.2",
    "echarts": "^6.0.0",
    "echarts-for-react": "^3.0.5",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^3.2.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "less": "^4.4.1",
    "less-loader": "^11.1.0",
    "lint-staged": "^16.2.7",
    "postcss-less": "^6.0.0",
    "prettier": "^3.3.3",
    "prism-react-renderer": "^2.4.0",
    "react-live": "^4.1.8",
    "style-loader": "^3.3.4",
    "stylelint": "^16.26.0",
    "stylelint-config-standard": "^39.0.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^5.4.10",
    "vitest": "^2.1.5"
  },
  "volta": {
    "node": "22.19.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{css,less}": [
      "stylelint --fix --quiet",
      "prettier --write",
      "git add"
    ]
  }
}
